{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "module": "es2020"
  },
  "files": ["main.ts"],
  "exclude": ["**/*.spec.ts"],
  "angularCompilerOptions": {
    "enableIvy": false,
    "preserveSymlinks": true,
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true,
    "preserveWhitespaces": true
  }
}
