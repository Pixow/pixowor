{
  "name": "Pixowor",
  "productName": "Pixowor",
  "version": "0.0.2",
  "description": "轻宇宙编辑器, 插件化架构设计",
  "main": "./dist/main.js",
  "repository": "git@github.com:Pixow/pixowor.git",
  "author": "Apowo Network Technology Co.,Ltd.",
  "license": "MIT",
  "scripts": {
    "ngcc": "ngcc",
    "start:develop": "npm-run-all -p electron:serve start:workbench:develop",
    "start:release": "npm-run-all -p electron:serve start:workbench:release",
    "start:production": "npm-run-all -p electron:serve start:workbench:productin",
    "electron:serve": "wait-on http-get://localhost:4301/ && npm run start:launcher:serve",
    "start:workbench:develop": "ng serve -c develop --port 4301",
    "start:workbench:release": "node --max-old-space-size=8192 node_modules/@angular/cli/bin/ng serve -c release --port 4301",
    "start:workbench:production": "ng serve -c production --port 4301",
    "start:launcher:serve": "rimraf dist/launcher && tsc -p tsconfig-launcher.json && npx electron . --serve",
    "build:launcher:tsc": "rimraf dist/launcher && tsc -p tsconfig-launcher.json",
    "build:launcher": "rimraf dist && npx webpack build --config ./launcher.webpack.js",
    "build:workbench:develop": "ng build workbench -c develop",
    "build:workbench:release": "ng build workbench -c release",
    "build:workbench:production": "ng build workbench -c production",
    "build:develop": "rimraf dist && npm run build:launcher:tsc && npm run build:workbench:develop",
    "build:release": "rimraf dist && npm run build:launcher:tsc && npm run build:workbench:release",
    "build:production": "rimraf dist && npm run build:launcher:tsc && npm run build:workbench:production",
    "build:windows:develop": "cross-env NODE_ENV=develop electron-builder build --windows --config=./build/scripts/build.js",
    "build:windows:release": "cross-env NODE_ENV=release electron-builder build --windows --config=./build/scripts/build.js",
    "build:windows:production": "cross-env NODE_ENV=production electron-builder build --windows --config=./build/scripts/build.js",
    "build:mac": "electron-builder build --osx --config=./build/scripts/build.js",
    "package:windows:develop": "npm run build:develop && npm run build:windows:develop",
    "package:windows:release": "npm run build:release && npm run build:windows:release",
    "package:windows:production": "npm run build:production && npm run build:windows:production",
    "e2e": "ng e2e"
  },
  "peerDependencies": {
    "@types/node": "^14.14.14"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^13.0.0",
    "@angular-devkit/build-angular": "^13.0.3",
    "@angular/animations": "^13.0.2",
    "@angular/cdk": "^13.0.2",
    "@angular/cli": "^13.0.3",
    "@angular/common": "^13.0.2",
    "@angular/compiler": "^13.0.2",
    "@angular/compiler-cli": "^13.0.2",
    "@angular/core": "^13.0.2",
    "@angular/elements": "^13.0.2",
    "@angular/forms": "^13.0.2",
    "@angular/platform-browser": "^13.0.2",
    "@angular/platform-browser-dynamic": "^13.0.2",
    "@angular/router": "^13.0.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@types/electron-json-storage": "^4.5.0",
    "@types/fs-extra": "^9.0.12",
    "@types/jasmine": "^3.6.2",
    "@types/jest": "^26.0.19",
    "@types/lodash-es": "^4.17.3",
    "@types/long": "^4.0.1",
    "@types/mocha": "^8.2.0",
    "axios": "^0.21.1",
    "chai": "^4.2.0",
    "compare-version": "^0.1.2",
    "copy-webpack-plugin": "6.3.2",
    "cross-env": "^7.0.3",
    "electron": "^11.2.1",
    "electron-builder": "^22.11.7",
    "game-capsule": "git+ssh://git@code.apowo.com:PixelPai/game-capsule.git#v2",
    "jasmine-core": "^3.6.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.15",
    "long": "^4.0.0",
    "moment": "^2.29.1",
    "monaco-editor": "^0.25.0",
    "ng-packagr": "^11.0.3",
    "ngx-build-plus": "^13.0.1",
    "npm-run-all": "^4.1.5",
    "primeflex": "^2.0.0",
    "primeicons": "^4.1.0",
    "protractor": "^7.0.0",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "systemjs": "^6.8.3",
    "ts-loader": "^9.2.5",
    "typescript": "4.4.2",
    "util": "^0.12.3",
    "wait-on": "^5.2.0",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-node-externals": "^3.0.0",
    "zone.js": "^0.11.3"
  },
  "dependencies": {
    "@PixelPai/game-core": "git+ssh://git@code.apowo.com:PixelPai/game-core.git#scene_editor",
    "@ngneat/transloco": "2.22.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-json-storage": "^4.5.0",
    "electron-log": "^4.4.1",
    "electron-re": "^1.0.13",
    "electron-updater": "^4.3.9",
    "fs-extra": "^10.0.0",
    "kill-port": "^1.6.1",
    "module-alias": "^2.2.2",
    "ngx-tippy-wrapper": "^4.0.1",
    "node-static": "^0.7.11",
    "pixowor-core": "git+ssh://git@github.com:Pixow/pixowor-core.git#develop",
    "primeng": "12.2.2",
    "qiniu": "^7.3.2",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0",
    "unzip-stream": "^0.3.1",
    "wget-improved": "^3.2.1",
    "zip-folder": "^1.0.0"
  }
}
