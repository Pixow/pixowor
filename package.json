{
  "name": "qing",
  "version": "0.0.1",
  "description": "图轻新版编辑器, 插件化架构设计",
  "main": "dist/main.js",
  "repository": "git@code.apowo.com:PixelPai/qing.git",
  "author": "<mojixiang@apowo.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm-run-all -p electron:serve ng:serve",
    "electron:serve": "wait-on tcp:4200 && npm run build:code && npx electron . --serve",
    "ng:serve": "ng serve -c dev",
    "build:workbench": "ng build workbench --base-href ./",
    "build:workbench:dev": "npm run build:workbench -- -c dev",
    "build:code": "tsc -p tsconfig.json",
    "build:extensions:element-editor": "ng build element-editor --base-href ./",
    "electron:local": "npm-run-all -p build:code build:workbench:dev && npx electron ."
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",
    "@angular-devkit/build-angular": "~0.1100.4",
    "@angular/cli": "^11.0.4",
    "@angular/common": "^11.0.4",
    "@angular/compiler": "^11.0.4",
    "@angular/compiler-cli": "~11.0.4",
    "@angular/core": "^11.0.4",
    "@angular/forms": "^11.0.5",
    "@angular/platform-browser": "^11.0.4",
    "@angular/platform-browser-dynamic": "^11.0.4",
    "@angular/router": "^11.0.4",
    "@ngxs/store": "^3.7.1",
    "@types/jasmine": "^3.6.2",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^14.14.14",
    "chai": "^4.2.0",
    "copyfiles": "^2.4.1",
    "electron": "^11.1.0",
    "jasmine-core": "^3.6.0",
    "lodash-es": "^4.17.15",
    "module-alias": "^2.2.2",
    "npm-run-all": "^4.1.5",
    "primeflex": "^2.0.0",
    "primeng": "^11.0.0",
    "typescript": "~4.0.2",
    "wait-on": "^5.2.0",
    "zone.js": "^0.11.3"
  },
  "_moduleAliases": {
    "src": "dist"
  },
  "dependencies": {
    "ngx-build-plus": "^10.1.1"
  }
}