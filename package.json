    "@types/node": "^14.14.13",
    "copyfiles": "^2.4.1",
    "electron": "^11.1.0",
    "lodash-es": "^4.17.15",
    "module-alias": "^2.2.2",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.0.2",
